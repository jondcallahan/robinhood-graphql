{
  "parser": "babel-eslint",
  "extends": ["eslint:recommended", "google"],
  "rules": {
    "camelcase": ["error", { "properties": "never" }],
    "object-curly-spacing": ["error", "always", { "objectsInObjects": false }],
    "arrow-parens": ["error", "as-needed"],
    "quotes": ["error", "single", { "avoidEscape": true }]
  },
  "env": {
    "es6": true,
    "node": true
  }
}
