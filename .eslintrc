{
  "parser": "babel-eslint",
  "extends": ["eslint:recommended", "google"],
  "rules": {
    "camelcase": ["error", {properties: "never"}],
    "semi": ["error", "never"],
    "object-curly-spacing": ["error", "always", { "objectsInObjects": false }]
    },
    "env": {
      "es6": true,
      "node": true
    },
}
